		<JavascriptInHead><![CDATA[function clearVote(buttonName){
	var buttonGroup = document.getElementsByName(buttonName);
	for (i=0; i < buttonGroup.length; i++) {
		if (buttonGroup[i].checked == true) {
			buttonGroup[i].checked = false;
       		}
   	}  
}
jQuery(document).ready(function(){
    jQuery('#ebSelectElection').bind('change',function(){
        jQuery('#ebSelectElectionForm').submit();;
    });
});]]></JavascriptInHead>
